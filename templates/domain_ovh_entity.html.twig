{#
/**
 * @file domain_ovh_entity.html.twig
 * Default theme implementation to present Domain Ovh Endpoint data.
 *
 * This template is used when viewing Domain Ovh Endpoint pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_domain_ovh_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('domain_ovh_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
